<h2><%= @entry.title %></h2>
  <p><%= @entry.date %></p>
  <p><%= @entry.contents %></p>
<p><%= link_to("Back to all entries", entries_path) %></p>

<%= link_to("New entry", new_entry_path) %>
<p><%= link_to("Edit this entry", edit_entry_path(@entry)) %></p>
<p><%= link_to("Delete this entry", entry_path(@entry), method: :delete) %></p>
